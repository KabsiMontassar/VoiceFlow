{
  "name": "@voiceflow/frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "VoiceFlow frontend application",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint --max-warnings 0",
    "lint:fix": "eslint --max-warnings 0 --fix",
    "format": "prettier \"src/**/*.{ts,tsx}\" --write",
